config BOARD_HALCYON_DONGLE
	select SOC_NRF52840_QIAA
	imply RETAINED_MEM if BOARD_HALCYON_DONGLE_NRF52840_ZMK
    imply RETENTION if BOARD_HALCYON_DONGLE_NRF52840_ZMK
    imply RETENTION_BOOT_MODE if BOARD_HALCYON_DONGLE_NRF52840_ZMK
